{
	"name": "react-demo",
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"rf": "rm -rf node_modules",
		"style": "stylelint 'src/**/*.(less)' --fix",
		"lint": "eslint '**/*.{ts,tsx}'",
		"lint:fix": "prettier -l --write './src/**/*.{ts,tsx,less,css}'",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
	},
	"dependencies": {
		"@ant-design/icons": "^4.7.0",
		"@pansy/china-division": "^2.0.0",
		"ahooks": "^3.3.10",
		"antd": "^4.21.7",
		"antd-mobile": "^5.24.0",
		"antd-mobile-icons": "^0.3.0",
		"dayjs": "^1.10.6",
		"flyio": "^0.6.14",
		"jairwin-utils": "^0.3.5",
		"lodash": "^4.17.21",
		"mockjs": "^1.1.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.3.0",
		"terser": "^5.14.2"
	},
	"devDependencies": {
		"@babel/core": "^7.14.6",
		"@babel/eslint-parser": "^7.16.3",
		"@babel/eslint-plugin": "^7.14.5",
		"@commitlint/cli": "^15.0.0",
		"@commitlint/config-angular": "^15.0.0",
		"@commitlint/config-conventional": "^15.0.0",
		"@types/mockjs": "^1.0.6",
		"@types/node": "^15.6.0",
		"@types/react": "^18.0.0",
		"@types/react-dom": "^18.0.0",
		"@types/react-router-dom": "^5.3.3",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"@umijs/fabric": "^2.8.1",
		"@vitejs/plugin-legacy": "^1.6.4",
		"eslint": "^8.3.0",
		"eslint-config-airbnb": "^19.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.27.1",
		"eslint-plugin-react-hooks": "^4.3.0",
		"husky": "^7.0.4",
		"less": "^4.1.1",
		"lint-staged": "^12.1.2",
		"prettier": "^2.3.2",
		"rollup-plugin-external-globals": "^0.6.1",
		"rollup-plugin-visualizer": "^5.5.2",
		"stylelint": "^14.1.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-recess-order": "^3.0.0",
		"stylelint-config-standard": "^24.0.0",
		"stylelint-declaration-block-no-ignored-properties": "^2.5.0",
		"stylelint-order": "^5.0.0",
		"typescript": "^4.5.4",
		"unplugin-auto-import": "^0.5.11",
		"vite": "^3.2.2",
		"vite-plugin-compression": "^0.3.0",
		"vite-plugin-externals": "^0.1.5",
		"vite-plugin-style-import": "^1.0.1"
	},
	"eslintConfig": {
		"extends": "react-app",
		"plugins": [
			"react-hooks"
		],
		"rules": {
			"react-hooks/rules-of-hooks": "error",
			"react-hooks/exhaustive-deps": "warn"
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "prettier --write . && git add . && git commit -am",
			"commit-msg": "commitlint -E",
			"pre-add": "lint-staged",
			"pre-push": "lint-staged"
		}
	},
	"lint-staged": {
		"src/*.{js,jsx,mjs,ts,tsx}": [
			"prettier --write",
			"eslint --fix",
			"git add",
			"git commit -am"
		]
	},
	"engines": {
    "node": ">=14"
  },
	"npm": {
		"mode": "yarn"
	},
	"cnpm": {
		"mode": "yarn"
	}
}
